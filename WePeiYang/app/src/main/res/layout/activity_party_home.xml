                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <Button
                            android:id="@+id/bt_home_5"
                            android:layout_width="wrap_content"
                            android:minWidth="180dp"
                            android:lines="1"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="15dp"
                            android:textColor="@color/myTextPrimaryColorGray"
                            android:background="@drawable/shape_button_home_gray"
                            android:text="递交第一季度思想汇报"/>
                        <Button
                            android:id="@+id/bt_home_6"
                            android:layout_width="wrap_content"
                            android:minWidth="180dp"
                            android:lines="1"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="11dp"
                            android:textColor="@color/myTextPrimaryColorGray"
                            android:background="@drawable/shape_button_home_gray"
                            android:text="递交第二季度思想汇报"/>
                        <Button
                            android:id="@+id/bt_home_7"
                            android:layout_width="wrap_content"
                            android:minWidth="180dp"
                            android:lines="1"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="11dp"
                            android:textColor="@color/myTextPrimaryColorGray"
                            android:background="@drawable/shape_button_home_gray"
                            android:text="递交第三季度思想汇报"/>

                        <Button
                            android:id="@+id/bt_home_8"
                            android:layout_width="wrap_content"
                            android:minWidth="180dp"
                            android:lines="1"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="11dp"
                            android:layout_marginBottom="11dp"
                            android:textColor="@color/myTextPrimaryColorGray"
                            android:background="@drawable/shape_button_home_gray"
                            android:text="递交第四季度思想汇报"/>
                    </LinearLayout>

                    <!--第一横向的第三竖排-->
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_marginLeft="14dp"
                        android:layout_marginRight="14dp">

                        <Button
                            android:id="@+id/bt_home_9"
                            android:layout_width="wrap_content"
                            android:minWidth="180dp"
                            android:lines="1"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="15dp"
                            android:textColor="@color/myTextPrimaryColorGray"
                            android:background="@drawable/shape_button_home_gray"
                            android:text="参加申请人学习小组"/>
                        <LinearLayout
