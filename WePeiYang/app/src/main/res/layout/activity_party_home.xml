            <!--第二个横着的部分-->
            <HorizontalScrollView
                android:id="@+id/hsv_2"
                android:layout_marginTop="24dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/bt_home_20"
                android:background="@color/myWindowBackgroundWhite"
