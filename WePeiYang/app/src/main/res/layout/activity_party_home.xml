                                android:layout_marginLeft="10dp"
                                android:lines="2"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="11dp"
                                android:textColor="@color/myTextPrimaryColorGray"
                                android:background="@drawable/shape_button_home_gray"
