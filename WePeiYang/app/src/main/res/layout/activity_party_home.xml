            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="党委谈话，审批"
            android:textColor="@color/myTextPrimaryColorGray"
            android:background="@drawable/shape_button_home_gray"
            android:layout_below="@+id/bt_home_18"
            android:layout_marginTop="15dp"
            android:layout_marginLeft="40dp"
            android:layout_marginRight="40dp"/>

            <View
                android:layout_width="1dp"
                android:layout_height="11dp"
                android:background="@color/myTextPrimaryColorGray"
                android:layout_marginTop="2dp"
                android:layout_marginBottom="2dp"
                android:layout_centerHorizontal="true"
                android:layout_below="@id/bt_home_19"/>

            <Button
                android:id="@+id/bt_home_20"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
