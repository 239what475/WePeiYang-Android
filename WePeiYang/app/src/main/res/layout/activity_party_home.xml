                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="支部向上级党组织汇报"
                android:textColor="@color/myTextPrimaryColorGray"
                android:background="@drawable/shape_button_home_gray"
                android:layout_below="@+id/bt_home_14"
                android:layout_marginTop="15dp"
                android:layout_marginLeft="40dp"
                android:layout_marginRight="40dp"/>

            <View
                android:layout_width="1dp"
                android:layout_height="11dp"
                android:background="@color/myTextPrimaryColorGray"
                android:layout_marginTop="2dp"
                android:layout_marginBottom="2dp"
                android:layout_centerHorizontal="true"
                android:layout_below="@id/bt_home_15"/>

            <Button
                android:id="@+id/bt_home_16"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="党员发展公示"
                android:textColor="@color/myTextPrimaryColorGray"
                android:background="@drawable/shape_button_home_gray"
                android:layout_below="@+id/bt_home_15"
                android:layout_marginTop="15dp"
                android:layout_marginLeft="40dp"
                android:layout_marginRight="40dp"/>

            <View
                android:layout_width="1dp"
                android:layout_height="11dp"
                android:background="@color/myTextPrimaryColorGray"
                android:layout_marginTop="2dp"
                android:layout_marginBottom="2dp"
                android:layout_centerHorizontal="true"
                android:layout_below="@id/bt_home_16"/>

            <Button
                android:id="@+id/bt_home_17"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="填写入党志愿书"
                android:textColor="@color/myTextPrimaryColorGray"
                android:background="@drawable/shape_button_home_gray"
                android:layout_below="@+id/bt_home_16"
                android:layout_marginTop="15dp"
                android:layout_marginLeft="40dp"
                android:layout_marginRight="40dp"/>

            <View
                android:layout_width="1dp"
                android:layout_height="11dp"
                android:background="@color/myTextPrimaryColorGray"
                android:layout_marginTop="2dp"
                android:layout_marginBottom="2dp"
                android:layout_centerHorizontal="true"
                android:layout_below="@id/bt_home_17"/>

            <Button
                android:id="@+id/bt_home_18"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="召开发展大会，党支部表决"
                android:textColor="@color/myTextPrimaryColorGray"
                android:background="@drawable/shape_button_home_gray"
                android:layout_below="@+id/bt_home_17"
                android:layout_marginTop="15dp"
                android:layout_marginLeft="40dp"
                android:layout_marginRight="40dp"/>

            <View
